@import 'common'
.cropper-wrapper
	position: relative
.prompt
	absolute: top 150px left 0px
	width: 200px
	margin-left: (@width / 2)
.router-link-active
	color: #f40
.shop-header
	background-color: #b9aebf
	padding-top:1rem
	.back
		padding:0px 10px
		a
			display: inline-block
		.iconfont
			color: #fff
.header-top
	height: auto
	clear: both
	overflow: hidden
	padding: 0.5rem 1rem
	color: #fff
	display: flex
	.login
		flex: 2
	img
		width: 100%
		border-radius: 20px
.header-top-cont
	flex: 5
	margin-left: 1rem
	h2
		font-size: 1.8rem
		line-height: 2rem
	p
		margin-top: 0.5rem
		line-height: 1.6rem
.more-supports
	float: right
	display: inline-block
	padding:0 1rem
	border-radius: 1rem
	background-color: rgba(0,0,0,0.2)
	color: #fff
.bulletin
	color: #fff
	padding: 0.5rem 1rem
	background-color: rgba(0,0,0,0.2)
	.more
		float: right
		color: #fff
.fade-enter-active
	animation: fade-in .5s

.fade-leave-active
	animation-name: fade-out .5s
	
.upfade-enter-active
	animation: upfade-in .5s
	
.upfade-leave-active
	animation-name: upfade-out .5s
	
@keyframes fade-in {
  0% {
    opacity: 0
  }
  100% {
    opacity: 1
  }
}
@keyframes fade-out {
  0% {
    opacity: 1
  }
  100% {
    opacity: 0
  }
}
@keyframes upfade-in {
  0% {
    bottom: 0
  }
  100% {
    bottom: 56px
  }
}
@keyframes upfade-out {
  0% {
    bottom: 56px
  }
  100% {
    bottom: 0
  }
}

.modal-wrapper
	position: fixed
	top: 0
	left: 0
	right: 0
	bottom: 0
	height: 100%
	width: 100%
	background-color: #262626
	color: #fff
	z-index: 4
.modal-cont
	padding: 0 4rem
	padding-top: 5rem
	h2
		font-size: 2rem
		text-align: center
	h3
		margin-top: 3.4rem
		margin-bottom: 2rem
		text-align: center
		span
			font-size: 1.4rem
			padding: 0.6rem 1rem
			border-radius: 0.8rem
			border: 1px solid #555
			background-color: #262626
	ul 
		li
			line-height: 1.8rem
			font-size: 1.4rem
	.rating-star
		display: inline-block
		position: relative
		height: 4rem
		line-height: 4rem
		span
			height: 4rem
		i
			font-size: 2.6rem
			float: left
			margin: 0px 4px
.modal-close
	position: fixed
	bottom: 2rem
	left: 50%
	margin-left: -1.25rem
	width: 3.5rem
	i
		font-size: 4rem
#product
	background-color: #fff
	display: flex
	position: absolute
	top: 227px
	bottom: 0
	width: 100%
	padding-bottom: 56px
	overflow: hidden
	.circleicon
		position: fixed
		color: #3190e8
		z-index: -1
		&.roll
			animation: circleroll 2s
		i
			font-size: 9.6rem
	.edit-num
		display: flex
		z-index: 2
		a
			display: inline-block
			color: #3190e8
			flex: 1
			background-color: #fff
			position: relative
			z-index: 1
			
		span
			display: inline-block
			flex: 1
			padding: 0 1rem
			line-height: 1.8rem
			text-align: center
		i
			font-size: 2.4rem
		
	footer
		position: absolute
		bottom: 0
		height: 56px
		width: 100%
		display: flex
		z-index: 3
		.cart
			flex: 3
			padding-left: 1rem
			background-color: #3d3d3f
			position: relative
			z-index: 3
			.icon
				display: inline-block
				width: 54px
				height: 54px
				position: absolute
				top: -1.4rem
				border: 0.4rem solid #444
				border-radius: 50%
				background-color: #535356
				color: #3d3d3f
				z-index: 2
				i
					font-size: 5.48rem
					line-height: 54px
				&.isbuy
					color: #3190e8
					background-color: #fff
				&.cart-icon-animate
					animation: carticon 0.3s
				em
					position: absolute
					right: -0.5rem
					top: -0.8rem
					display: inline-block
					background-color: #ff461d
					border-radius: 50%
					color: #fff
					width: 1.5rem
					padding: 3px
					text-align: center
					letter-spacing: 0
			.price
				display: inline-block
				color: #fff
				margin-left: 5.4rem
				font-size: 1.4rem
				padding-top: 0.8rem
				em
					font-size: 1.2rem
					display:block		
		.gobuy
			flex: 1
			background-color: #444
			color: #fff
			line-height: @height
			text-align: center
			font-size: 1.6rem
			position: relative
			z-index: 3
			&.isbuy
				background-color: #4cd964
		.cart-list
			position: absolute
			bottom: 56px
			width: 100%
			z-index: 1
			.title
				padding: 0.6rem 1rem
				background-color: #eceff1
				height: 1.6rem
				line-height: 1.6rem
				span
					display: inline-block
					font-size: 1.4rem
					padding-left: 0.6rem
					border-left: 0.3rem solid #3190e8
				a
					float: right
					font-size: 1.4rem
					i
						float: left
						font-size: 1.8rem
						margin-right: 0.1rem
						margin-top: 0.1rem
			ul
				background-color: #fff
				li
					padding: 1.2rem 1rem
					font-size: 1.4rem
					display: flex
					&:not(:last-child)
						border-bottom: 1px solid #eee
					h3
						flex: 6
					.price
						flex: 2
						color: #f60
						font-size: 1.6rem
					.edit-num
						flex: 2

@keyframes carticon
{
	0% {
		transform: scale(1, 1)
	}
	50% {
		transform: scale(0.8, 0.8)
	}
	100% {
		transform: scale(1, 1)
	}
}
@-webkit-keyframes carticon
{
	0% {
		transform: scale(1, 1)
	}
	50% {
		transform: scale(0.8, 0.8)
	}
	100% {
		transform: scale(1, 1)
	}
}		
.model-view
	position: fixed
	top: 0
	bottom: 0
	left: 0
	right: 0
	width: 100%
	height: 100%
	z-index: 2
	background-color: rgba(0, 0, 0, 0.3)
			
.shop-bar
	background-color: #fff
	position: relative
	z-index: 2
	display: flex
	li
		background-color: #fff
		text-align: center
		height: 3rem
		line-height: 3rem
		font-size: 1.6rem
		border-bottom: 1px solid #ddd
		flex:1
	li.active 
		a
			color: #f40
.product-nav-list
	width: 6.5rem
	float: left
	li
		width: 100%
		background-color: #f8f8f8
		a
			display: block
			padding: 1rem 0
			margin: 0 0.8rem
			border-bottom: 1px solid #ededed
			font-size: 1.2rem
			color: #666
	li.cur
		background-color: #fff
		position: relative
		&:after
			content: ""
			position: absolute
			top: 0
			left: 0
			height: 100%
			width: 3px
			background-color: #3190e8
		a
			border-bottom-color: #fff
.productList
	flex:1
.product-item-list
	width: 100%
	li
		margin: 0 1rem
		padding: 1rem 0
		border-bottom:1px solid #ddd
		position: relative
		display: flex
		img
			width: 5rem
			height: 5rem
			float: left
	.food-cont
		margin-left: 1rem
		flex:1
		h4
			font-size: 1.4rem
		p
			color: #666
			margin-top: 3px
		.price strong
			color: #f40
			font-size: 1.8rem
		.price span
			text-decoration: line-through
	.edit-num
		position: absolute
		bottom: 0.6rem
		right: 0

.rating-cont
	padding: 1rem
	display: flex
	margin-bottom: 1rem
	background-color: #fff
	position: relative
	z-index: 2
	.left
		padding-right: 1rem
		border-right: 1px solid #ddd
		text-align: center
		line-height: 1.5
		strong
			font-size: 1.8rem
			color: #f60
			line-height: 2.2rem
	.right
		flex:1
		li
			display: flex
			line-height: 1.8rem
		.title
			margin-left: 1rem
		.score
			color: #f60
			margin-left: 1rem
		.star-five
			margin-right: 5px
.rating-list
	background-color: #fff
	padding: 0 1rem
	height: 340px
	> ul 
		> li
			padding: 1rem 0
			border-bottom: 1px solid #ddd
			position: relative
	ul 
		li 
			.userimg
				width: 4rem
				height: 4rem
				border-radius: 50%
				float: left
	.rating-user-info
		margin-left: 5rem
		font-size: 1rem
		h4
			font-size: 2rem
			line-height: 2rem
		p
			margin-bottom: 8px
		.foods 
			span
				display: inline-block
				color: #999
				border:1px solid #ddd
				padding: 0 0.3rem
				border-radius: 0.3rem
				margin-right: 5px
				margin-bottom: 5px
				float: left
	.date
		position: absolute
		top: 1rem
		right: 0
#rating 
	.rating-star
		display: inline-block
		position: relative
		height: 1.8rem
		line-height: 1.8rem
		span
			display: inline-block
			height: 1.8rem
		i
			font-size: 1rem
			float: left
			margin: 0px 1px

.header
	padding: 10px
	height: 2rem
	background-color: rgb(0, 151, 255)
	position: relative
	z-index: 9 
	.back
		position: absolute
		top: 6px
		display: inline-block
		line-height: 3rem
		
	.iconfont
		color: #fff
	.title
		display: block
		line-height: @height
		width: 12rem
		margin: 0 auto
		text-align: center
		font-size: 1.8rem
		color: #fff
.seller-list
	background-color: #fff
	li
		padding: 1rem
		border-bottom: 1px solid #eee
		a
			display: flex
		.list-logo
			flex: 1
			img
				width: 100%
		.list-info
			flex: 4
			margin-left: 1rem
			letter-spacing: 0
			h2
				font-size: 1.4rem
			.info-item
				margin-top: 0.8rem
				display: flex
				.divider
					margin: 0 0.3rem
					color: #ddd
				.mark
					span
						display: inline-block
						padding: 0 0.3rem
						color: #2395ff
						border: 1px solid #44a5ff
						border-radius: 0.2rem
						margin:0 0.1rem
				p
					flex: 1
					color: #666
				.tr
					text-align: right
.seller-filter
	position: relative
	z-index: 3
	ul
		border-bottom: 1px solid #ddd
		display: flex
		background-color: #fff
	li
		flex: 1
		height: 3rem
		a
			display: block
			height: 3rem
			line-height: 3rem
			font-size: 1.4rem
			text-align: center
			color: #666
			&.cur
				color: #2395ff
				i
					display: inline-block
					transform: rotate(180deg)
					transition: transform 0.3s
					color: #2395ff
		i
			color: #999
			font-size: 2rem
	.filter
		position: absolute
		top: 3.1rem
		left: 0
		width: 100%
		background-color: #fff
		.search
			padding: 10px
			display: flex
			input
				flex: 1
				height: 2.8rem
				border: 1px solid #ddd
				font-size: 1.4rem
				text-align: center
				border-radius: 0.6rem
						